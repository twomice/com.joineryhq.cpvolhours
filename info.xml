<?xml version="1.0"?>
<extension key="com.joineryhq.cpvolhours" type="module">
  <file>cpvolhours</file>
  <name>CarePartners Volunteer Hours</name>
  <description>Custom for CarePartners: batch entry of Volunteer/Team hours.</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://github.com/twomice/com.joineryhq.cpvolhours</url>
    <url desc="Documentation">http://github.com/twomice/com.joineryhq.cpvolhours</url>
    <url desc="Support">http://github.com/twomice/com.joineryhq.cpvolhours/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2021-04-20</releaseDate>
  <version>1.1</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Developed for Care Partners Texas by Joinery. This extension assumes specific CiviCRM configurations which it does not define.</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Cpvolhours</namespace>
  </civix>
</extension>
